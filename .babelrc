{
  "plugins": [
    "transform-class-properties",
    "transform-react-jsx",
    "transform-runtime",
    "transform-export-extensions",
    "syntax-dynamic-import",
    "lodash"
  ],
  "presets": [
    [
      "babel-preset-env",
      {
        "targets": {
          "ie": 9,
          "uglify": true
        }
      }
    ],
    "babel-preset-react"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "babel-preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "babel-preset-react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-react-jsx",
        "transform-runtime",
        "transform-export-extensions",
        "dynamic-import-node",
        "lodash"
      ]
    },
    "build": {
      "ignore": [
        "*.test.js"
      ]
    }
  }
}
